<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ale.misactivos">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icono"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Vistas.MenuCrudsActivity"></activity>
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Vistas.CreateAccountActivity"
            android:parentActivityName=".LoginActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".LoginActivity" />
        </activity>
        <activity
            android:name=".RefActivity"
            android:label="@string/title_activity_ref"
            android:theme="@style/AppTheme" />
        <activity android:name=".Vistas.ContainerActivity" />
        <activity android:name=".Vistas.MenuInicialActivity" />
        <activity android:name=".Vistas.GestionServidorActivity" />
        <activity android:name=".Vistas.ObtenerDataActivity" />
        <activity android:name=".Vistas.CreateActivoActivity" />
        <activity android:name=".Vistas.GestionActivosActivity" />
        <activity android:name=".Operaciones.CrudLogin" />
        <activity android:name=".Vistas.EdificiosActivity" />
        <activity android:name=".Vistas.GestionCrudsActivity" />
        <activity android:name=".Vistas.ProfesionesActivity" />
    </application>

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>